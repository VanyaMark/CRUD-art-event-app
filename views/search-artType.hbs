<div class="inner-body-div">
    <h2>Artwork By Type:</h2>
    {{#each applicationsArray}}
    <hr>
    <h6>Exhibition Week:</h6>
    <p>{{this.chooseWeek}}</p>
    <h6>Artwork Title:</h6>
    <p>{{this.artworkName}}</p>
    <h6>Art Type:</h6>
    <p>{{this.artType}}</p>
    <img class="ex-pic" src="{{this.artworkUrl}}">
    <p>By: {{this.firstName}} {{this.lastName}}</p>
    <h6>Description:</h6>
    <p>{{this.description}}</p>

    {{/each}}
</div>

<div>
    {{>buttonsPartial}}
</div>

