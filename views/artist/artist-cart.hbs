
{{#if artistApp}}

<img src="{{artistApp.profilePicUrl}}">
    <p>First Name: {{artistApp.firstName}}</p>
    <p>Last Name: {{artistApp.lastName}}</p>
    <p>Email: {{artistApp.email}}</p>

    <p>Phone Number: {{artistApp.phoneNumber}}</p>
    <p>Address: {{artistApp.address}}</p>
    <p>Date of Birth: {{artistApp.dateOfBirth}}</p>

    <p>Artwork Image URL</p>
    <img src="{{artistApp.artworkUrl}}">
    <p>Art Type: {{artistApp.artType}}</p>
    <p>Wall Size: {{artistApp.wallSize}}</p>
    <p>Description: {{artistApp.description}}</p>
    <p>Requested Date: {{artistApp.dateRequested}}</p>

     <button><a href="/artistCart/{{artistApp._id}}/edit">Edit Application Details</a></button>


{{/if}}


    {{#each editedItem}}

    <img src="{{this.profilePicUrl}}">
    <p>First Name: {{this.firstName}}</p>
    <p>Last Name: {{this.lastName}}</p>
    <p>Email: {{this.email}}</p>

    <p>Phone Number: {{this.phoneNumber}}</p>
    <p>Address: {{this.address}}</p>
    <p>Date of Birth: {{this.dateOfBirth}}</p>

    <p>Artwork Image URL</p>
    <img src="{{this.artworkUrl}}">
    <p>Art Type: {{this.artType}}</p>
    <p>Wall Size: {{this.wallSize}}</p>
    <p>Description: {{this.description}}</p>
    <p>Requested Date: {{this.dateRequested}}</p>



    <button><a href="/artistCart/{{this._id}}/edit">Edit Application Details</a></button>

{{/each}}


  

